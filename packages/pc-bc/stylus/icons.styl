@font-face 
  font-family 'can-pc'  /* project id 847196 */
  src url('//at.alicdn.com/t/font_847196_hyza3dd0u38.eot')
  src url('//at.alicdn.com/t/font_847196_hyza3dd0u38.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_847196_hyza3dd0u38.woff') format('woff'),
  url('//at.alicdn.com/t/font_847196_hyza3dd0u38.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_847196_hyza3dd0u38.svg#can-pc') format('svg')


.cp-icon
  direction ltr
  display inline-block
  font normal normal normal $fs-xxxl "can-pc"
  font-size inherit
  line-height inherit
  letter-spacing normal
  text-rendering auto
  text-transform none
  white-space nowrap
  overflow-wrap normal
  -webkit-font-feature-settings "liga"
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

  &before
    font-size $fs-xxxl

  &--wechatbefore
    content "\e694"

  &--qqbefore
    content "\e6db"
  
  &--weibobefore
    content "\e61c"
